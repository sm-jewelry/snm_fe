# Next.js Frontend Environment Variables
# Copy this file to .env.local and fill in the actual values

# API Gateway URL
NEXT_PUBLIC_API_GATEWAY_URL=http://localhost:8000

# Razorpay Configuration (Public Key Only)
NEXT_PUBLIC_RAZORPAY_KEY=rzp_test_your_public_key

# Google OAuth (if using client-side)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Analytics (Optional)
NEXT_PUBLIC_GA_TRACKING_ID=G-XXXXXXXXXX
